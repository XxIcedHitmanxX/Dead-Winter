		local cframe = game.Players.LocalPlayer.Character["Left Leg"].CFrame
for i,v in pairs(game.Workspace.vehicles.Car1:GetDescendants()) do
	if v:IsA("Part") then
		v.CFrame = cframe + Vector3.new(20, 0, 0)
	end
end
